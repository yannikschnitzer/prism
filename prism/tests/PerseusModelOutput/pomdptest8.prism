pomdp
observables v, h endobservables
module vehicle
// vehicle location
     v: [0..12] init 12;
	// v=12 initial dummy variable
    [c0] v=12 -> (v'=0);
	// v=0
	[c0] v=0 -> (v'=1);
	[c1] v=0 -> (v'=2);
	[c2] v=0 -> (v'=3);
	// v=1
	[c1] v=1 -> (v'=4);
	[c2] v=1 -> (v'=5);
	// v=2
	[c0] v=2 -> (v'=4);
	[c2] v=2 -> (v'=6);
	// v=3
	[c0] v=3 -> (v'=5);
	[c1] v=3 -> (v'=6);
	// v=4
	[c1] v=4 -> (v'=7);
	[c2] v=4 -> (v'=8);
	// v=5
	[c0] v=5 -> (v'=7);
	[c2] v=5 -> (v'=9);
	// v=6
	[c0] v=6 -> (v'=8);
	[c1] v=6 -> (v'=9);
	// v=7
	[c1] v=7 -> (v'=10);
	// v=8
	[c1] v=8 -> (v'=10);
	// v=9
	[c1] v=9 -> (v'=10);
	// v=10
	[c0] v=10 -> (v'=11);
	[c1] v=10 -> (v'=11);
	[c2] v=10 -> (v'=11);
	// v=11
	[c0] v=11 -> (v'=11);
	[c1] v=11 -> (v'=11);
	[c2] v=11 -> (v'=11);

endmodule

module driver
u: [0..1] init 0;
h: [0..2] init 0;

[c0] u =0 -> 0.05: (h'=1)& (u'=0) + 0.15: (h'=1)& (u'=1) + 0.35: (h'=2)& (u'=0) + 0.45: (h'=2)& (u'=1);
[c1] u =0 -> 0.05: (h'=1)& (u'=0) + 0.15: (h'=1)& (u'=1) + 0.35: (h'=2)& (u'=0) + 0.45: (h'=2)& (u'=1);
[c2] u =0 -> 0.05: (h'=1)& (u'=0) + 0.15: (h'=1)& (u'=1) + 0.35: (h'=2)& (u'=0) + 0.45: (h'=2)& (u'=1);

[c0] u =1 -> 0.05: (h'=1)& (u'=0) + 0.15: (h'=1)& (u'=1) + 0.35: (h'=2)& (u'=0) + 0.45: (h'=2)& (u'=1);
[c1] u =1 -> 0.05: (h'=1)& (u'=0) + 0.15: (h'=1)& (u'=1) + 0.35: (h'=2)& (u'=0) + 0.45: (h'=2)& (u'=1);
[c2] u =1 -> 0.05: (h'=1)& (u'=0) + 0.15: (h'=1)& (u'=1) + 0.35: (h'=2)& (u'=0) + 0.45: (h'=2)& (u'=1);

endmodule

label "end" = v=11;

rewards "min_takeoverTimes_V2"
h=1:1;
endrewards