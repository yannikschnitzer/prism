module control
    c : [0..1] init 0;
    [move] c = 0 -> 1: (c'=1);
    [north] c = 0 -> 1: (c'=1);
    [south] c = 0 -> 1: (c'=1);
    [east] c = 0 -> 1: (c'=1);
    [west] c = 0 -> 1: (c'=1);
    [up] c = 0 -> 1: (c'=1);
    [down] c = 0 -> 1: (c'=1);
    [end] c = 0 -> 1: (c'=1);
    [env] c = 1 -> 1: (c'=0);
    [] attarget -> true;
endmodule

