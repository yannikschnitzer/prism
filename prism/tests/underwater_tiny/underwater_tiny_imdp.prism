mdp

const double e = 0.15;

module M

	s : [0..3] init 0;

	[n] s=0 -> [0.65-e, 0.65+e] : (s'=2) + [(1-0.65)-e,(1-0.65)+e] : (s'=0);
	[e] s=0 -> 1 : (s'=1) ;

    [n] s=1 -> [0.65-e, 0.65+e] : (s'=3) +  [(1-0.65)-e,(1-0.65)+e] : (s'=1);
	[w] s=1 -> 1 : (s'=0) ;

    [e] s=2 -> [0.65-e, 0.65+e] : (s'=3) +  [(1-0.65)-e,(1-0.65)+e] : (s'=1);
	[s] s=2 -> 1 : (s'=0) ;

	[] s=3 -> true;

endmodule

label "goal" = s=3;
label "obs" = s=2;

rewards
	s=0 : 1;
	s=1 : 1;
	s=2 : 1;
	s=3 : 0;
endrewards