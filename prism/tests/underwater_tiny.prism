mdp

const double p;

module M

	s : [0..3] init 0;

	[N] s=0 -> 0.5 : (s'=3) + 0.5 : (s'=1);
	[E] s=0 -> 1 : (s'=2) ;

    [N] s=1 -> p : (s'=3) + 1-p : (s'=1);
	[W] s=1 -> 1 : (s'=0) ;

    [E] s=2 -> p : (s'=3) + 1-p : (s'=1);
	[S] s=2 -> 1 : (s'=0) ;

	[] s=3 -> true;

endmodule

label "goal" = s=3;
//label "obs" = s=2;

rewards
	s=0 : 1;
	s=1 : 1;
	s=2 : 1;
	s=3 : 0;
endrewards