mdp
const int N=30; // N*N grid map
const double p=0.1; // with probability p, a move to a neighboring state state occurs due to seeing and control noise

module robot
	//define robot position
	r:[1..N] init 1; //grid row
	c:[1..N] init 1; //grid column

	// go to the terminal state when hitting an obstacle
	[obstacle] (r=1 & c=12) | (r=4 & c=27) | (r=4 & c=18) | (r=28 & c=16) | (r=30 & c=11) | (r=9 & c=6) | (r=11 & c=3) | (r=26 & c=7) | (r=16 & c=21) | (r=15 & c=17) | (r=2 & c=15) | (r=21 & c=22) | (r=26 & c=25) | (r=10 & c=14) | (r=17 & c=12) | (r=12 & c=27) | (r=17 & c=6) | (r=27 & c=16) | (r=24 & c=21) | (r=24 & c=24) | (r=14 & c=15) | (r=5 & c=4) | (r=13 & c=21) | (r=8 & c=7) | (r=5 & c=3) | (r=28 & c=28) | (r=15 & c=1) | (r=4 & c=4) | (r=2 & c=29) | (r=27 & c=2)-> (r'=30) & (c'=1);

	// transitions
	[east] (c<N | c=N) & !((r=1 & c=12) | (r=4 & c=27) | (r=4 & c=18) | (r=28 & c=16) | (r=30 & c=11) | (r=9 & c=6) | (r=11 & c=3) | (r=26 & c=7) | (r=16 & c=21) | (r=15 & c=17) | (r=2 & c=15) | (r=21 & c=22) | (r=26 & c=25) | (r=10 & c=14) | (r=17 & c=12) | (r=12 & c=27) | (r=17 & c=6) | (r=27 & c=16) | (r=24 & c=21) | (r=24 & c=24) | (r=14 & c=15) | (r=5 & c=4) | (r=13 & c=21) | (r=8 & c=7) | (r=5 & c=3) | (r=28 & c=28) | (r=15 & c=1) | (r=4 & c=4) | (r=2 & c=29) | (r=27 & c=2))-> p/3:(r'=min(r+1, N)) + (1-p): (c'=min(c+1, N))+ p/3: (c'=max(c-1, 1))+ p/3: (r'=max(r-1, 1));
	[south] (r<N | r=N) & !((r=1 & c=12) | (r=4 & c=27) | (r=4 & c=18) | (r=28 & c=16) | (r=30 & c=11) | (r=9 & c=6) | (r=11 & c=3) | (r=26 & c=7) | (r=16 & c=21) | (r=15 & c=17) | (r=2 & c=15) | (r=21 & c=22) | (r=26 & c=25) | (r=10 & c=14) | (r=17 & c=12) | (r=12 & c=27) | (r=17 & c=6) | (r=27 & c=16) | (r=24 & c=21) | (r=24 & c=24) | (r=14 & c=15) | (r=5 & c=4) | (r=13 & c=21) | (r=8 & c=7) | (r=5 & c=3) | (r=28 & c=28) | (r=15 & c=1) | (r=4 & c=4) | (r=2 & c=29) | (r=27 & c=2))-> (1-p):(r'=min(r+1, N)) + (p)/3: (c'=max(c-1, 1))+ (p)/3: (r'=max(r-1, 1))+ p/3: (c'=min(c+1, N));
	[west]  (c>1 | c=1) & !((r=1 & c=12) | (r=4 & c=27) | (r=4 & c=18) | (r=28 & c=16) | (r=30 & c=11) | (r=9 & c=6) | (r=11 & c=3) | (r=26 & c=7) | (r=16 & c=21) | (r=15 & c=17) | (r=2 & c=15) | (r=21 & c=22) | (r=26 & c=25) | (r=10 & c=14) | (r=17 & c=12) | (r=12 & c=27) | (r=17 & c=6) | (r=27 & c=16) | (r=24 & c=21) | (r=24 & c=24) | (r=14 & c=15) | (r=5 & c=4) | (r=13 & c=21) | (r=8 & c=7) | (r=5 & c=3) | (r=28 & c=28) | (r=15 & c=1) | (r=4 & c=4) | (r=2 & c=29) | (r=27 & c=2))-> p/3:(r'=max(r-1, 1)) + (1-p): (c'=max(c-1, 1))+ p/3: (c'=min(c+1, N))+ p/3: (r'=min(r+1, N));
	[north] (r=1 | r>1) & !((r=1 & c=12) | (r=4 & c=27) | (r=4 & c=18) | (r=28 & c=16) | (r=30 & c=11) | (r=9 & c=6) | (r=11 & c=3) | (r=26 & c=7) | (r=16 & c=21) | (r=15 & c=17) | (r=2 & c=15) | (r=21 & c=22) | (r=26 & c=25) | (r=10 & c=14) | (r=17 & c=12) | (r=12 & c=27) | (r=17 & c=6) | (r=27 & c=16) | (r=24 & c=21) | (r=24 & c=24) | (r=14 & c=15) | (r=5 & c=4) | (r=13 & c=21) | (r=8 & c=7) | (r=5 & c=3) | (r=28 & c=28) | (r=15 & c=1) | (r=4 & c=4) | (r=2 & c=29) | (r=27 & c=2))-> (1-p):(r'=max(r-1, 1)) + p/3: (c'=min(c+1, N))+ p/3: (r'=min(r+1, N))+ p/3: (c'=max(c-1, 1));

	// terminal state self-loop to avoid deadlock
	[] r=30 & c=1 -> true;
endmodule

label "goal" = r=30 & c=1;

rewards
	[east] true : 1;
	[south] true : 1;
	[west] true : 1;
	[north] true : 1;
	[obstacle] true : 70.0;
endrewards
