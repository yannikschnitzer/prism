mdp
const int N=305; // N*N grid map
const double p=0.1; // with probability p, a move to a neighboring state state occurs due to seeing and control noise

module robot
	//define robot position
	r:[1..N] init 1; //grid row
	c:[1..N] init 1; //grid column

	// go to the terminal state when hitting an obstacle
	[obstacle] (r=244 & c=61) | (r=245 & c=55) | (r=92 & c=55) | (r=68 & c=45) | (r=132 & c=50) | (r=95 & c=74) | (r=144 & c=131) | (r=102 & c=35) | (r=167 & c=191) | (r=108 & c=184) | (r=138 & c=18) | (r=232 & c=119)-> (r'=305) & (c'=1);

	// transitions
	[east] (c<N | c=N) & !((r=244 & c=61) | (r=245 & c=55) | (r=92 & c=55) | (r=68 & c=45) | (r=132 & c=50) | (r=95 & c=74) | (r=144 & c=131) | (r=102 & c=35) | (r=167 & c=191) | (r=108 & c=184) | (r=138 & c=18) | (r=232 & c=119))-> p/3:(r'=min(r+1, N))+ p/3: (c'=max(c-1, 1))+ p/3: (r'=max(r-1, 1)) + (1-p): (c'=min(c+1, N));
	[south] (r<N | r=N) & !((r=244 & c=61) | (r=245 & c=55) | (r=92 & c=55) | (r=68 & c=45) | (r=132 & c=50) | (r=95 & c=74) | (r=144 & c=131) | (r=102 & c=35) | (r=167 & c=191) | (r=108 & c=184) | (r=138 & c=18) | (r=232 & c=119))-> (1-p):(r'=min(r+1, N)) + p/3: (c'=min(c+1, N)) + p/3:(r'=min(r+1, N))+ p/3: (c'=max(c-1, 1));
	[west]  (c>1 | c=1) & !((r=244 & c=61) | (r=245 & c=55) | (r=92 & c=55) | (r=68 & c=45) | (r=132 & c=50) | (r=95 & c=74) | (r=144 & c=131) | (r=102 & c=35) | (r=167 & c=191) | (r=108 & c=184) | (r=138 & c=18) | (r=232 & c=119))-> p/3:(r'=max(r-1, 1))+ p/3:(r'=min(r+1, N))+ p/3: (c'=min(c+1, N)) + (1-p): (c'=max(c-1, 1));
	[north] (r=1 | r>1) & !((r=244 & c=61) | (r=245 & c=55) | (r=92 & c=55) | (r=68 & c=45) | (r=132 & c=50) | (r=95 & c=74) | (r=144 & c=131) | (r=102 & c=35) | (r=167 & c=191) | (r=108 & c=184) | (r=138 & c=18) | (r=232 & c=119))-> (1-p):(r'=max(r-1, 1)) + p/3: (c'=max(c-1, 1)) + p/3: (c'=min(c+1, N)) + p/3:(r'=min(r+1, N));

	// terminal state self-loop to avoid deadlock
	[] r=305 & c=1 -> true;
endmodule

label "goal" = r=305 & c=1;

rewards
	[east] true : 1;
	[south] true : 1;
	[west] true : 1;
	[north] true : 1;
	[obstacle] true : 60.0;
endrewards
