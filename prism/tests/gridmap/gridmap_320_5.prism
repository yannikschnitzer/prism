mdp
const int N=320; // N*N grid map
const double p=0.1; // with probability p, a move to a neighboring state state occurs due to seeing and control noise

module robot
	//define robot position
	r:[1..N] init 1; //grid row
	c:[1..N] init 1; //grid column

	// go to the terminal state when hitting an obstacle
	[obstacle] (r=236 & c=10) | (r=247 & c=49) | (r=133 & c=27) | (r=241 & c=36) | (r=205 & c=106) | (r=159 & c=113) | (r=147 & c=10) | (r=72 & c=14) | (r=184 & c=30) | (r=249 & c=191) | (r=100 & c=256) | (r=75 & c=249)-> (r'=320) & (c'=1);

	// transitions
	[east] (c<N | c=N) & !((r=236 & c=10) | (r=247 & c=49) | (r=133 & c=27) | (r=241 & c=36) | (r=205 & c=106) | (r=159 & c=113) | (r=147 & c=10) | (r=72 & c=14) | (r=184 & c=30) | (r=249 & c=191) | (r=100 & c=256) | (r=75 & c=249))-> p/3:(r'=min(r+1, N))+ p/3: (c'=max(c-1, 1))+ p/3: (r'=max(r-1, 1)) + (1-p): (c'=min(c+1, N));
	[south] (r<N | r=N) & !((r=236 & c=10) | (r=247 & c=49) | (r=133 & c=27) | (r=241 & c=36) | (r=205 & c=106) | (r=159 & c=113) | (r=147 & c=10) | (r=72 & c=14) | (r=184 & c=30) | (r=249 & c=191) | (r=100 & c=256) | (r=75 & c=249))-> (1-p):(r'=min(r+1, N)) + p/3: (c'=min(c+1, N)) + p/3:(r'=min(r+1, N))+ p/3: (c'=max(c-1, 1));
	[west]  (c>1 | c=1) & !((r=236 & c=10) | (r=247 & c=49) | (r=133 & c=27) | (r=241 & c=36) | (r=205 & c=106) | (r=159 & c=113) | (r=147 & c=10) | (r=72 & c=14) | (r=184 & c=30) | (r=249 & c=191) | (r=100 & c=256) | (r=75 & c=249))-> p/3:(r'=max(r-1, 1))+ p/3:(r'=min(r+1, N))+ p/3: (c'=min(c+1, N)) + (1-p): (c'=max(c-1, 1));
	[north] (r=1 | r>1) & !((r=236 & c=10) | (r=247 & c=49) | (r=133 & c=27) | (r=241 & c=36) | (r=205 & c=106) | (r=159 & c=113) | (r=147 & c=10) | (r=72 & c=14) | (r=184 & c=30) | (r=249 & c=191) | (r=100 & c=256) | (r=75 & c=249))-> (1-p):(r'=max(r-1, 1)) + p/3: (c'=max(c-1, 1)) + p/3: (c'=min(c+1, N)) + p/3:(r'=min(r+1, N));

	// terminal state self-loop to avoid deadlock
	[] r=320 & c=1 -> true;
endmodule

label "goal" = r=320 & c=1;

rewards
	[east] true : 1;
	[south] true : 1;
	[west] true : 1;
	[north] true : 1;
	[obstacle] true : 60.0;
endrewards
